{
    "name": "renegare/skip",
    "description": "Simple configuration layer around silex framework. With the aim of reducing spin up times with 'green field' projects.",
    "keywords": ["silex", "config", "renegare"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mudi Ugbowanko",
            "email": "mudi@ugbowanko.com"
        }
    ],

    "minimum-stability": "dev",

    "autoload": {
        "psr-0": {
            "Skip": "src"
        }
    },

    "require": {
        "symfony/finder": ">=2.3,<2.5-dev",
        "silex/silex": ">=1.0",
        "seld/jsonlint": "1.1.2",
        "symfony/console": "v2.4.1"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "symfony/browser-kit": ">=2.3,<2.5-dev"
    }
}
