{
    "name": "renegare/skip",
    "description": "Simple configuration layer around silex framework. With the aim of reducing spin up times with 'green field' projects.",
    "keywords": ["silex", "config", "renegare"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mudi Ugbowanko",
            "email": "mudi@ugbowanko.com"
        }
    ],

    "minimum-stability": "dev",

    "autoload": {
        "psr-0": {
            "Skip": "src"
        }
    },

    "require": {
        "silex/silex": "v1.1.2",
        "symfony/finder": "v2.4.1",
        "jdorn/sql-formatter": "v1.2.0"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "symfony/browser-kit": "v2.4.1"
    }
}
